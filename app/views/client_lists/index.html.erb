<h1>Listing Clients</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_lists.each do |client_list| %>
      <tr id="<%= dom_id(client_list) %>">
        <td><%= client_list.client %></td>
        <td><%= client_list.description %></td>
        <td>
          <%= link_to "Client Dashboard", client_list_prospects_path(client_list) %>
          <%= link_to 'Add Prospect', new_client_list_prospect_path(client_list) %>
          <%= link_to 'Edit', edit_client_list_path(client_list) %>
          <%= link_to 'Destroy', client_list, method: :delete, data: { confirm: 'Are you sure?' } %>



        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client list', new_client_list_path %>
